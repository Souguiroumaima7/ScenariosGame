<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <!-- Status Bar -->

    <LinearLayout
        android:id="@+id/statusbar1"
        android:layout_width="425dp"
        android:layout_height="33dp"
        android:layout_marginTop="9dp"
        android:layout_marginBottom="710dp"
        android:background="@color/transparent"
        android:gravity="center"
        android:padding="40dp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,UselessParent,VisualLintBounds">

        <!-- Add status bar content here, e.g., text, icons, etc. -->
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:text="Status Bar Content"
            android:textColor="@android:color/white"
            android:textSize="16sp"
            android:visibility="visible"
            tools:ignore="HardcodedText,TextContrastCheck,VisualLintBounds"
            tools:visibility="visible" />

    </LinearLayout>

    <ImageView
        android:id="@+id/menu"
        android:layout_width="28dp"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="96dp"
        android:layout_marginBottom="8dp"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toTopOf="@+id/updown"
        app:layout_constraintEnd_toStartOf="@+id/location"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/statusbar1"
        app:layout_constraintVertical_bias="0.68"
        app:srcCompat="@drawable/menu_svgrepo_com"
        tools:ignore="MissingConstraints,VisualLintBounds" />

    <TextView
        android:id="@+id/location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="16dp"
        android:text="Location"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/paris"
        app:layout_constraintEnd_toStartOf="@+id/updown"
        tools:ignore="HardcodedText,MissingConstraints,VisualLintBounds,VisualLintOverlap" />

    <TextView
        android:id="@+id/paris"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="170dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="774dp"
        android:text="Paris"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/updown"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/updown"
        tools:ignore="HardcodedText,MissingConstraints,VisualLintBounds,VisualLintOverlap" />

    <ImageView
        android:id="@+id/updown"
        android:layout_width="27dp"
        android:layout_height="18dp"
        android:layout_marginStart="16dp"
        android:contentDescription="TODO"
        app:layout_constraintBottom_toTopOf="@+id/paris"
        app:layout_constraintStart_toEndOf="@+id/paris"
        app:layout_constraintTop_toBottomOf="@+id/menu"
        app:srcCompat="@drawable/back_up_down_svgrepo_com"
        tools:ignore="ContentDescription,HardcodedText,MissingConstraints,VisualLintBounds" />

    <TextView
        android:id="@+id/Howdoesitwork"
        android:layout_width="194dp"
        android:layout_height="35dp"
        android:layout_marginBottom="446dp"
        android:fontFamily="sans-serif-medium"
        android:text="@string/how_does_it_work"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:drawableEndCompat="@drawable/arrow_forward_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/statusbar1"
        app:layout_constraintVertical_bias="0.997"
        tools:ignore="TextSizeCheck,VisualLintOverlap" />

    <ImageView
        android:id="@+id/Arrow"
        android:layout_width="78dp"
        android:layout_height="39dp"
        android:layout_marginStart="17dp"
        android:layout_marginBottom="469dp"
        android:contentDescription="@string/arrow_"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Howdoesitwork"
        app:layout_constraintTop_toBottomOf="@+id/statusbar1"
        app:layout_constraintVertical_bias="0.984"
        app:srcCompat="@drawable/forward_arrow" />

    <Button
        android:id="@+id/share"
        android:layout_width="88dp"
        android:layout_height="55dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/rectangleshape"
        android:text="@string/share"
        app:layout_constraintBaseline_toBaselineOf="@+id/ReadTOS"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/ReadTOS"
        android:layout_width="99dp"
        android:layout_height="54dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/rectangleshape"
        android:text="@string/read_tos"
        app:layout_constraintStart_toEndOf="@+id/share"
        app:layout_constraintTop_toBottomOf="@+id/Howdoesitwork" />

    <Button
        android:id="@+id/Confidentiality"
        android:layout_width="142dp"
        android:layout_height="52dp"
        android:layout_marginStart="25dp"
        android:background="@drawable/rectangleshape"
        android:text="@string/confidentiality"
        app:layout_constraintBaseline_toBaselineOf="@+id/ReadTOS"
        app:layout_constraintStart_toEndOf="@+id/ReadTOS"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/aboutus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="296dp"
        android:text="@string/about_us"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/statusbar1"
        app:layout_constraintVertical_bias="0.991" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="336dp"
        android:layout_height="98dp"
        android:layout_marginStart="44dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="164dp"
        android:text="@string/lorem_ipsum_dolor_sit_amet_consectetur_a_fermentm_faucibus_netus_morbi_nisl_varius_turpis_purus_eget_vol_amet_mi_laoreet_rhoncus"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.615"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/statusbar1"
        app:layout_constraintVertical_bias="0.986"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="127dp"
        android:text="@string/read_more"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/statusbar1" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="78dp"
        android:layout_height="30dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="116dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="112dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/updown"
        app:layout_constraintHorizontal_bias="0.278"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/arrow_forward_icon"
        android:contentDescription="@string/image" />

    <View
        android:id="@+id/group_10"
        android:layout_width="403dp"
        android:layout_height="304dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="3dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/share"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="395dp"
        android:layout_height="212dp"
        android:layout_marginBottom="2dp"
        android:contentDescription="@string/imageview__"
        app:layout_constraintBottom_toTopOf="@+id/Arrow"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/_photo_foreground" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="104dp"
        android:layout_height="82dp"
        android:layout_marginStart="42dp"
        android:layout_marginBottom="11dp"
        app:layout_constraintBottom_toTopOf="@+id/group_10"
        app:layout_constraintStart_toEndOf="@+id/updown"
        app:srcCompat="@mipmap/viewim_photo_foreground" />


</androidx.constraintlayout.widget.ConstraintLayout>