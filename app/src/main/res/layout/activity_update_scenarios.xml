            android:layout_marginBottom="108dp"
            app:layout_constraintBottom_toTopOf="@+id/plus"
            app:layout_constraintEnd_toEndOf="parent">

        <TextView
                android:id="@+id/Scenarios2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="88dp"
                android:layout_marginTop="16dp"
                android:text="@string/scenarios"
                android:textAlignment="textStart"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:visibility="visible"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginBottom="3dp"
                app:layout_constraintBottom_toTopOf="@+id/Timer1" />

        <ImageView
                android:id="@+id/imageView8"
                android:layout_width="82dp"
                android:layout_height="109dp"
                android:contentDescription="@string/__img_"
                app:srcCompat="@mipmap/icon"
                tools:ignore="VisualLintBounds"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="6dp"
                app:layout_constraintBottom_toBottomOf="parent" />

        <ImageView
                android:id="@+id/Timer1"
                android:layout_width="17dp"
                android:layout_height="25dp"
                android:contentDescription="@string/todo"
                app:srcCompat="@drawable/timer_svgrepo_com"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="13dp"
                android:layout_marginStart="9dp"
                app:layout_constraintStart_toEndOf="@+id/imageView8" />

        <TextView
                android:id="@+id/hour2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/___1_hr"
                android:textColor="#0277BD"
                app:layout_constraintStart_toEndOf="@+id/Timer1"
                app:layout_constraintTop_toTopOf="@+id/Timer1"
                app:layout_constraintBottom_toBottomOf="@+id/Timer1"
                android:layout_marginStart="12dp"
                app:layout_constraintVertical_bias="0.19" />

        <Button
                android:layout_width="29dp"
                android:layout_height="36dp"
                android:id="@+id/edit3"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="24dp"
                app:layout_constraintEnd_toStartOf="@+id/delete3"
                android:layout_marginEnd="40dp"
                android:background="@drawable/edit_2_svgrepo_com"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <Button
                android:layout_width="35dp"
                android:layout_height="38dp"
                android:id="@+id/delete3"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="24dp"
                android:layout_marginEnd="32dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:background="@drawable/delete_3_svgrepo_com"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
            android:layout_width="24dp"
            android:layout_height="21dp"
            android:id="@+id/backNav"
            android:background="@drawable/navigation_back_arrow_svgrepo_com"
            tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"
            android:layout_marginBottom="213dp"
            app:layout_constraintBottom_toTopOf="@+id/relativeLayout"
            app:layout_constraintEnd_toStartOf="@+id/scenarios"
            android:layout_marginEnd="84dp" />

    <Button
            android:layout_width="68dp"
            android:layout_height="47dp"
            android:id="@+id/plus"
            android:layout_marginBottom="40dp"
            app:layout_constraintBottom_toTopOf="@+id/button4"
            android:layout_marginEnd="160dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:background="@drawable/_491254405_plusaddmoredetail_82972"
            tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>