<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/relativeLayout"
            android:layout_width="422dp"
            android:layout_height="75dp"
            android:background="#FFFFFF"
            tools:ignore="DuplicateIds,MissingConstraints,VisualLintBounds"

            android:layout_marginBottom="64dp"
            app:layout_constraintBottom_toTopOf="@+id/relativeLayout1"
            app:layout_constraintEnd_toEndOf="parent">

        <TextView
                android:id="@+id/Scenarios1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="88dp"
                android:layout_marginTop="16dp"
                android:text="@string/scenarios"
                android:textAlignment="textStart"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:visibility="visible"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        <ImageView
                android:id="@+id/imageView7"
                android:layout_width="82dp"
                android:layout_height="109dp"
                android:contentDescription="@string/img_"
                app:srcCompat="@mipmap/icon"
                tools:ignore="VisualLintBounds"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="4dp"
                app:layout_constraintBottom_toBottomOf="parent" />

        <ImageView
                android:id="@+id/imageView9"
                android:layout_width="17dp"
                android:layout_height="25dp"
                android:contentDescription="@string/___todo"
                app:srcCompat="@drawable/timer_svgrepo_com"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="9dp"
                app:layout_constraintEnd_toStartOf="@+id/hour1"
                android:layout_marginEnd="7dp" />

        <TextView
                android:id="@+id/hour1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/_1_hr"
                android:textColor="#0277BD"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="13dp"
                app:layout_constraintEnd_toStartOf="@+id/abedit"
                android:layout_marginEnd="138dp" />

        <Button
                android:layout_width="32dp"
                android:layout_height="33dp"
                android:id="@+id/abedit"
                android:background="@drawable/edit_2_svgrepo_com"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="26dp"
                app:layout_constraintEnd_toStartOf="@+id/delete1"
                android:layout_marginEnd="40dp"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <Button
                android:layout_width="33dp"
                android:layout_height="34dp"
                android:id="@+id/delete1"
                android:background="@drawable/delete_3_svgrepo_com"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="27dp"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
                android:layout_marginEnd="34dp"
                app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/relativeLayout"
            android:layout_width="412dp"
            android:layout_height="77dp"
            android:background="#FFFFFF"
            tools:ignore="DuplicateIds,MissingConstraints,VisualLintBounds"

            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="4dp"
            android:layout_marginTop="95dp"
            app:layout_constraintTop_toBottomOf="@+id/scenarios">

        <TextView
                android:id="@+id/Scenarios1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="88dp"
                android:layout_marginTop="16dp"
                android:text="@string/scenarios"
                android:textAlignment="textStart"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:visibility="visible"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        <ImageView
                android:id="@+id/imageView7"
                android:layout_width="73dp"
