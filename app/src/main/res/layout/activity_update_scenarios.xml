                android:layout_height="31dp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="16dp"
                app:layout_constraintEnd_toStartOf="@+id/delete1"
                android:layout_marginEnd="32dp"
                android:background="@drawable/edit_2_svgrepo_com"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"
                android:layout_marginStart="125dp"
                app:layout_constraintStart_toEndOf="@+id/Scenarios1"
                app:layout_constraintHorizontal_bias="1.0"
                android:id="@+id/edit1" />

        <Button
                android:layout_width="25dp"
                android:layout_height="31dp"
                android:id="@+id/delete1"
                android:layout_marginEnd="34dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="18dp"
                android:background="@drawable/delete_3_svgrepo_com"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
            android:id="@+id/linearLayout10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/transparent"
            android:gravity="center"
            android:padding="8dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="0dp">

        <!-- Add status bar content here, e.g., text, icons, etc. -->
        <TextView
                android:id="@+id/statusbar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Status Bar Content"
                android:textColor="@android:color/white"
                android:textSize="16sp"
                tools:ignore="HardcodedText,TextContrastCheck" />
    </LinearLayout>

    <TextView
            android:id="@+id/scenarios"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/scenarios"
            android:textColor="#130000"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="57dp"
            tools:layout_editor_absoluteX="139dp"
            android:textSize="20sp" />

    <Button
            android:id="@+id/button4"
            android:layout_width="252dp"
            android:layout_height="0dp"
            android:background="@drawable/roundshapebtn"
