<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <!-- Status Bar -->

    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/linearLayout6"
            android:layout_width="432dp"
            android:layout_height="313dp"
            android:background="@color/Extraviolet"
            android:padding="40dp"
            tools:ignore="MissingConstraints"
            android:orientation="horizontal"
            tools:layout_editor_absoluteY="-12dp"
            tools:layout_editor_absoluteX="-3dp">

        <Button
                android:text="@string/save"
                android:layout_width="116dp"
                android:layout_height="73dp"
                android:id="@+id/save"
                tools:ignore="TouchTargetSizeCheck,TextContrastCheck"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="299dp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="149dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:background="@color/transparent" />

        <TextView
                android:text="@string/profile__"
                android:layout_width="85dp"
                android:layout_height="55dp"
                android:id="@+id/profile"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="179dp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="56dp"
                android:layout_marginEnd="132dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="95dp"
                android:textSize="20sp"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintVertical_bias="0.0"
                tools:ignore="TextContrastCheck" />

        <ImageView
                android:layout_width="87dp"
                android:layout_height="72dp"
                app:srcCompat="@mipmap/viewim_photo_foreground"
                android:id="@+id/imageView"
                tools:layout_editor_absoluteY="169dp"
                tools:layout_editor_absoluteX="133dp"
                android:contentDescription="@string/viewimage" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <EditText
            android:layout_width="349dp"
            android:layout_height="53dp"
            android:inputType="textMultiLine"
            android:ems="10"
            android:id="@+id/editTextUsername"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="36dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="450dp"
            android:background="@color/white"
            tools:ignore="LabelFor,SpeakableTextPresentCheck"
            android:autofillHints="" />

    <TextView
            android:text="@string/username_"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/username"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="33dp"
            android:textSize="16sp"
            android:textColor="@color/black"
            android:layout_marginBottom="22dp"
            app:layout_constraintBottom_toTopOf="@+id/editTextUsername" />

    <TextView
            android:text="@string/email"
            android:layout_width="47dp"
            android:layout_height="32dp"
            android:id="@+id/emailadress"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="400dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="36dp"
            android:textSize="16sp"
            android:textColor="@color/black"
            tools:ignore="TextSizeCheck" />

    <EditText
            android:layout_width="352dp"
            android:layout_height="56dp"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/editTextTextEmailAdress"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="332dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="36dp"
            android:textColor="@color/black"
            tools:ignore="LabelFor,SpeakableTextPresentCheck"
            android:autofillHints=""
            android:background="@color/white" />

    <TextView
            android:text="@string/gender"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Gender"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="292dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp"
            android:textSize="16sp"
            android:textColor="@color/black" />

    <EditText
            android:layout_width="351dp"
            android:layout_height="54dp"
            android:inputType="textMultiLine"
            android:gravity="start|top"
            android:ems="10"
            android:id="@+id/editTextTextMultiLine"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="220dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="36dp"
            android:background="@color/white"
            tools:ignore="LabelFor,SpeakableTextPresentCheck"
            android:minHeight="48dp"
            android:autofillHints="" />

    <TextView
            android:text="@string/phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/phone"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="188dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="36dp"
            android:textSize="16sp"
            android:textColor="@color/black" />

    <EditText
            android:layout_width="346dp"
            android:layout_height="51dp"
            android:inputType="phone"
            android:ems="10"
            android:id="@+id/editTextPhone"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="118dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="33dp"
            android:background="@color/white"
            tools:ignore="LabelFor,SpeakableTextPresentCheck"
            android:autofillHints="" />

    <TextView
            android:text="City"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/State"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="29dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="86dp"
            android:textSize="16sp"
            android:textColor="@color/black" />

    <EditText
            android:layout_width="346dp"
            android:layout_height="54dp"
            android:inputType="textMultiLine"
            android:gravity="start|top"
            android:ems="10"
            android:id="@+id/CityTextEdit"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="31dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="22dp"
            android:background="@color/white"
            tools:ignore="LabelFor,SpeakableTextPresentCheck"
            android:autofillHints="" />

</androidx.constraintlayout.widget.ConstraintLayout>
